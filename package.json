{
  "name": "mobx-react-lite",
  "version": "1.2.0",
  "description": "Lightweight React bindings for MobX based on React 16.8 and Hooks",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.module.js",
  "module": "dist/index.module.js",
  "react-native": "dist/native.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-lite.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "lint": "tslint --project .",
    "validate": "tsc --noEmit",
    "test": "jest --watch",
    "test:travis": "yarn validate && yarn lint && yarn coverage",
    "coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "node build-rollup.js"
  },
  "author": "Daniel K.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-react-lite/issues"
  },
  "homepage": "https://mobxjs.github.io/mobx",
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "coveralls": "3.0.3",
    "husky": "1.3.1",
    "jest": "24.7.1",
    "jest-dom": "3.1.3",
    "jest-environment-jsdom": "24.7.1",
    "jest-mock-console": "0.4.2",
    "lint-staged": "8.1.5",
    "lodash": "4.17.11",
    "mobx": "5.9.4",
    "prettier": "1.16.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-testing-library": "6.0.4",
    "rimraf": "2.6.3",
    "rollup": "1.8.0",
    "rollup-plugin-alias": "1.5.1",
    "rollup-plugin-commonjs": "9.3.3",
    "rollup-plugin-filesize": "6.0.1",
    "rollup-plugin-node-resolve": "4.0.1",
    "rollup-plugin-replace": "2.1.1",
    "rollup-plugin-terser": "4.0.4",
    "rollup-plugin-typescript2": "0.20.1",
    "ts-jest": "24.0.1",
    "tslint": "5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.1"
  },
  "keywords": [
    "mobx",
    "mobservable",
    "react-component",
    "react",
    "reactjs",
    "reactive",
    "hooks",
    "observer",
    "useObserver",
    "useComputer"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {}
}
