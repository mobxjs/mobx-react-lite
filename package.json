{
  "name": "mobx-react-lite",
  "version": "0.2.0",
  "description": "Lightweight React bindings for MobX based on experimental React hooks",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.module.js",
  "module": "dist/index.module.js",
  "react-native": "dist/native.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-lite.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "lint": "tslint --project .",
    "validate": "tsc --noEmit --pretty",
    "test": "jest --watch",
    "test:travis": "yarn lint && jest",
    "build": "node build-rollup.js",
    "release": "np"
  },
  "author": "Daniel K.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-react-lite/issues"
  },
  "homepage": "https://mobxjs.github.io/mobx",
  "peerDependencies": {
    "mobx": "^5.0.0",
    "react": "^16.7.0-alpha.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.0.0",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "jest-environment-jsdom": "^23.4.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "mobx": "^5.0.0",
    "np": "^3.0.4",
    "opn-cli": "^3.1.0",
    "prettier": "^1.15.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "request": "^2.88.0",
    "rollup": "^0.67.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "shelljs": "^0.8.2",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "keywords": [
    "mobx",
    "mobservable",
    "react-component",
    "react",
    "reactjs",
    "reactive"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
