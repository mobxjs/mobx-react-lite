{
  "name": "mobx-react-lite",
  "version": "1.4.0",
  "description": "Lightweight React bindings for MobX based on React 16.8 and Hooks",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.module.js",
  "module": "dist/index.module.js",
  "react-native": "dist/native.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-lite.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "lint": "tslint --project .",
    "validate": "tsc --noEmit",
    "test": "jest --watch",
    "test:travis": "yarn validate && yarn lint && yarn coverage",
    "coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "node build-rollup.js",
    "prepublishOnly": "yarn build"
  },
  "author": "Daniel K.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx-react-lite/issues"
  },
  "homepage": "https://mobxjs.github.io/mobx",
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "coveralls": "3.0.4",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "jest-environment-jsdom": "24.8.0",
    "jest-mock-console": "1.0.0",
    "lint-staged": "8.2.1",
    "lodash": "4.17.11",
    "mobx": "5.10.1",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-hooks-testing-library": "0.5.1",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "7.0.1",
    "rimraf": "2.6.3",
    "rollup": "1.15.3",
    "rollup-plugin-alias": "1.5.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-filesize": "6.1.0",
    "rollup-plugin-node-resolve": "5.0.2",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.0.0",
    "rollup-plugin-typescript2": "0.21.1",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  },
  "keywords": [
    "mobx",
    "mobservable",
    "react-component",
    "react",
    "reactjs",
    "reactive",
    "hooks",
    "observer",
    "useObserver",
    "useComputer"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {}
}
