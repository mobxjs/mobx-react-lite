{
  "name": "mobx-react-lite",
  "version": "0.0.1",
  "description": "React bindings for MobX. Create fully reactive components.",
  "main": "index.js",
  "jsnext:main": "index.module.js",
  "module": "index.module.js",
  "react-native": "native.js",
  "typings": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-lite.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "test": "jest && npm run test:ts",
    "test:ts": "tsc -p test/ts",
    "test:travis": "npm run build && jest && npm run test:ts",
    "build": "node build-rollup.js && shx cp src/index.d.ts index.d.ts && shx cp src/index.d.ts native.d.ts && shx cp src/index.d.ts custom.d.ts",
    "watch": "jest --watch",
    "lint": "tslint --project ."
  },
  "author": "Michel Weststrate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "https://mobxjs.github.io/mobx",
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0",
    "react": "^16.7.alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@types/create-react-class":"^15.6.2",
    "@types/node": "^10.0.0",
    "@types/react": "^16.7.2",
    "@types/react-dom": "^16.0.9",
    "babel-core": "bridge",
    "babel-jest": "^23.6.0",
    "create-react-class": "^15.6.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "mobx": "^5.0.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.15.1",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "request": "^2.88.0",
    "rollup": "^0.67.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "shelljs": "^0.8.2",
    "shx": "^0.3.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "keywords": [
    "mobx",
    "mobservable",
    "react-component",
    "react",
    "reactjs",
    "reactive"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
